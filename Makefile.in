$O/misc.o: misc.c config.h
	${MPICC} $(CFLAGS) -c $(PYTHON_CFLAGS) -o $O/misc.o misc.c

$O/PyxParameters.o: PyxParameters.c config.h
	${CC} -c $(CFLAGS) $(PYTHON_CFLAGS) -o $O/PyxParameters.o PyxParameters.c

