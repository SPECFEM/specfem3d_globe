xcombine_vol_data: $O/combine_vol_data.o $O/write_c_binary.o
	${FCCOMPILE_CHECK} -o xcombine_vol_data  $O/combine_vol_data.o $O/write_c_binary.o

xcombine_surf_data: $O/combine_surf_data.o $O/write_c_binary.o
