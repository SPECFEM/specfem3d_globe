#!/bin/sh

# @configure_input@

while test $# != 0
do
  case $1 in
  --fclibs)
     fclibs="@FCLIBS@"
     ;;
  --python-cppflags)
     python_cppflags="-I@PYTHON_INCDIR@"
     ;;
  --python-ldflags)
     python_ldflags="@PYTHON_LDFLAGS@ @PYTHON_LINKFORSHARED@ @PYTHON_BLDLIBRARY@ @PYTHON_LIBS@ @PYTHON_MODLIBS@ @PYTHON_SYSLIBS@ @PYTHON_LDLAST@"
     ;;
  esac
  shift
done

echo $link $python_cppflags $fclibs $python_ldflags

# end of file
